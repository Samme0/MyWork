@model Assigntment4WebApp.ViewModel.OrderEditViewModel //<<<<<<< can't figure out way tis not working

@{
    ViewData["Title"] = "Edit Order";
}

<h1>Edit Order</h1>

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="fooOrder.BeverageItem"></label>
        <select asp-for="fooOrder.BeverageItem" class="form-control" asp-items="Model.BeverageList"></select>
        <span asp-validation-for="fooOrder.BeverageItem" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="fooOrder.AppetizerItem"></label>
        <select asp-for="fooOrder.AppetizerItem" class="form-control" asp-items="Model.AppetizerList"></select>
        <span asp-validation-for="fooOrder.AppetizerItem" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="fooOrder.SandwichItem"></label>
        <select asp-for="fooOrder.SandwichItem" class="form-control" asp-items="Model.SandwichList"></select>
        <span asp-validation-for="fooOrder.SandwichItem" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="fooOrder.DessertItem"></label>
        <select asp-for="fooOrder.DessertItem" class="form-control" asp-items="Model.DessertList"></select>
        <span asp-validation-for="fooOrder.DessertItem" class="text-danger"></span>
    </div>

    <input type="hidden" asp-for="fooOrder.OrderId" />

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Save</button>
        <a asp-action="Delete" asp-route-id="@Model.fooOrder.OrderId" class="btn btn-danger">Delete</a>
        <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    </div>
</form>

<div>
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @if (!string.IsNullOrEmpty(Model.Message))
    {
        <div class="alert alert-warning">@Model.Message</div>
    }
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}